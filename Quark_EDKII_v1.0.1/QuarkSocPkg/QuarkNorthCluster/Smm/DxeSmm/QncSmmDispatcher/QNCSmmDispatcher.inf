  QNCSmmHelpers.h
  QNCxSmmHelpers.h
  QNCSmmRegisters.h
  QNCSmm.h
  CommonHeader.h

[Packages]
  MdePkg/MdePkg.dec
  QuarkSocPkg/QuarkSocPkg.dec
  MdeModulePkg/MdeModulePkg.dec
  IntelFrameworkPkg/IntelFrameworkPkg.dec

[LibraryClasses]
  UefiDriverEntryPoint
  SmmServicesTableLib
  UefiBootServicesTableLib
  DxeServicesTableLib
  MemoryAllocationLib
  PciLib
  PcdLib
  BaseMemoryLib
  DebugLib
  BaseLib
  IoLib
  DevicePathLib
  S3IoLib
