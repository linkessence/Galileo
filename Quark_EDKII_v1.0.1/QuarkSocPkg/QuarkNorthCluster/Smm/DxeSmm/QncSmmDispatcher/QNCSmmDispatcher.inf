[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = QNCSmmDispatcher
  FILE_GUID                      = 2480271C-09C6-4f36-AD75-5E1390BD9929
  MODULE_TYPE                    = DXE_SMM_DRIVER
  VERSION_STRING                 = 1.0
  PI_SPECIFICATION_VERSION       = 0x0001000A
