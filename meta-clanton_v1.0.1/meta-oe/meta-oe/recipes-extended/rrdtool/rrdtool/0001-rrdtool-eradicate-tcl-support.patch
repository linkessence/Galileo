-
-	# get the two interface's octet counter values
-	set in [lindex [lindex [$session get IF-MIB!ifInOctets.$inst] 0] 2]
-	set out [lindex [lindex [$session get IF-MIB!ifOutOctets.$inst] 0] 2]
